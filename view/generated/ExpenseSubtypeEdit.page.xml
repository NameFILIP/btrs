<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/ExpenseSubtypeList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{expenseSubtypeHome.wire}"/>

   <param name="expenseSubtypeFrom"/>
   <param name="expenseSubtypeId" value="#{expenseSubtypeHome.expenseSubtypeId}"/>
   <param name="expenseTypeFrom"/>
   <param name="expenseTypeId" value="#{expenseTypeHome.expenseTypeId}"/>



   <navigation from-action="#{expenseSubtypeHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/ExpenseSubtype.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{expenseSubtypeHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/ExpenseSubtype.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{expenseSubtypeHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/ExpenseSubtypeList.xhtml"/>
      </rule>
   </navigation>

</page>
