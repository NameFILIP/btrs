<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/ExpenseList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{expenseHome.wire}"/>

   <param name="expenseFrom"/>
   <param name="expenseId" value="#{expenseHome.expenseId}"/>
   <param name="expenseTypeFrom"/>
   <param name="expenseTypeId" value="#{expenseTypeHome.expenseTypeId}"/>

   <param name="reportFrom"/>
   <param name="reportId" value="#{reportHome.reportId}"/>
   <param name="statusChangeFrom"/>
   <param name="statusChangeId" value="#{statusChangeHome.statusChangeId}"/>
   <param name="statusFrom"/>
   <param name="statusId" value="#{statusHome.statusId}"/>

   <param name="userFrom"/>
   <param name="userId" value="#{userHome.userId}"/>
   <param name="departmentFrom"/>
   <param name="departmentId" value="#{departmentHome.departmentId}"/>

   <param name="roleFrom"/>
   <param name="roleId" value="#{roleHome.roleId}"/>






   <navigation from-action="#{expenseHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/Expense.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{expenseHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/Expense.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{expenseHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/ExpenseList.xhtml"/>
      </rule>
   </navigation>

</page>
